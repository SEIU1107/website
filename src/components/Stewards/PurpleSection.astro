---
export interface Props {
    title: string;
}

const { title = "" } = Astro.props as Props;
---

<div class="bg-gradient-to-b from-honey-flower-800 to-honey-flower-900">
    <div class="text-white font-bold font-Trebuchet text-center py-10 text-5xl">
        {title}
    </div>
    <slot />
</div>

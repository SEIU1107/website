        <Image src={logoPLAN} alt="" />
        <Image src={logoPlannedParenthood} alt="" />
        <Image src={logoSSV} alt="" />
        <Image src={logoSSE} alt="" />
        <Image src={logoNAACP} alt="" />
        <Image src={logoRichWellnessCollective} alt="" />
      </ul>
      <div
        class="text-black font-Trebuchet text-center font-bold text-3xl py-3"
      >
        Full List of Event Partners
      </div>
      <ul class="ml-5 list-disc grid gap-5 grid-cols-1 md:grid-cols-2">
        <li>ACLU</li>
        <li>African Chamber of Commerce and Tourism</li>
        <li>African Diaspora of Las Vegas (ADOLV)</li>

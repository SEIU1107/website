<Layout title = "SEIU Local 1107 Staff">
    <HeadingTitle text="SEIU Local 1107 Staff"/>
    <StaffGrid data={profileData} className="staff-grid"/>
</Layout>

<style>
    .staff-grid {
        display: grid;
        grid-template-columns: repeat(4,1fr);
        grid-column-gap: 0;
        margin: auto;
        justify-content: center;
        justify-items: center;
        max-width: 375px;
        padding: 10px;
        font-family: 'Roboto', sans-serif;
    }

    @media (max-width: 1400px) {
        .staff-grid {
            grid-template-columns: repeat(3,1fr);
        }
    }

    @media (max-width: 767px) {
        .staff-grid {
            grid-template-columns: 1fr;
        }
    }
</style>
---
import Layout from "../../layouts/Layout.astro";
import ProfileGrid from "../../components/ProfileGrid.astro";
import HeadingTitle from "../../components/HeadingTitle.astro";
import type { Profile } from "../../Profile";

import NicoleCourts from "../../images/executive_board/Nicole.png";

const data: Profile[] = [
    {
        name: "Nicole Courts",
        profileImg: NicoleCourts,
        data: [
            ["Title", "SEIU 1107 Executive Board Member"],
            ["Department", "RN / General Surgical"],
            ["Phone", "(775) 843 - 8898"],
            ["Email", "nicolecourts1@gmail.com"],
        ],
    },
];
---

<Layout
    title="Renown Stewards"
    backgroundStyling="bg-gradient-to-b from-honey-flower-100 to-honey-flower-300"
>
    <HeadingTitle text="Renown Hospital" />
    <ProfileGrid data={data} />
</Layout>
